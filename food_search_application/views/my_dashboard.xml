<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="menu_dashboard" model="website.menu">
        <field name="name">Dashboard</field>
        <field name="url">/my/dashboard</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">30</field>
    </record>

    <template id="my_dashboard" name="My Dashboard">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <div class="row mt-4">
                        <div class="col-sm-4">
                            <div class="card">
                                <div class="card-body">
                                    <h5 class="card-title">Product Details All</h5>
                                    <a href="/all_product_list_view" class="btn btn-primary">View</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-sm-4">
                            <div class="card">
                                <div class="card-body">
                                    <h5 class="card-title">Price Range</h5>
                                    <a href="/test" class="btn btn-primary">View</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

</odoo>